{
  "name": "tm/new-php-project",
  "description": "My personal template for a new PHP project.",
  "type": "project",
  "keywords": [
    "key",
    "word"
  ],
  "homepage": "https://github.com/tommy-muehle/php-repository-starter",
  "authors": [
    {
      "name": "Tommy Muehle",
      "email": "tommy.muehle@gmail.com",
      "homepage": "https://tommy-muehle.de"
    }
  ],
  "support": {
    "email": "tommy.muehle@gmail.com",
    "issues": "https://github.com/tommy-muehle/php-repository-starter/issues",
    "docs": "https://github.com/tommy-muehle/php-repository-starter/blob/master/README.md",
    "source": "https://github.com/tommy-muehle/php-repository-starter/tree/master"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "New\\Project\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "New\\Project\\": "src/",
      "New\\Project\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6.0"
  },
  "config": {
    "bin-dir": "bin",
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
